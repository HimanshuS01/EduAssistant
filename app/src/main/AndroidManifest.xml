        android:theme="@style/MyMaterialTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".StudentListRecyclerView" />
        <activity android:name=".ChatbotFiles.ChatActivity">



        </activity>
        <activity
            android:name=".Initial.SplashScreen"
            android:theme="@style/SplashTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".ExpandTextView" />
        <activity android:name=".SpeechToText.STT"></activity>
        <activity android:name=".SpeechToText.SttDisplay"></activity>






        <activity android:name=".Initial.MainScreen"></activity>


